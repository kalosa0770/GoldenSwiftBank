
{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
     
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}